<project xmlns="http://maven.apache.org/DECORATION/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0
    http://maven.apache.org/xsd/decoration-1.0.0.xsd"
    name="ymock">

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-stylus-skin</artifactId>
        <version>1.2</version>
    </skin>

    <bannerLeft>
        <name>yMock</name>
        <src>images/logo.png</src>
        <href>http://www.ymock.com/</href>
    </bannerLeft>

    <poweredBy>
        <logo name="Maven" href="http://maven.apache.org/"
            img="http://maven.apache.org/images/logos/maven-feather.png"/>
    </poweredBy>

    <body>
        <head>
            <link href="/images/favicon.gif" rel="shortcut icon" />
            <link href="/css/ymock.css" rel="stylesheet" type="text/css" />
        </head>
        <breadcrumbs>
            <item name="ymock.com" href="http://www.ymock.com"/>
        </breadcrumbs>
        <links>
            <item name="Built with Maven 2" href="http://maven.apache.org/" />
        </links>
        <menu name="Overview">
            <item name="Introduction" href="./index.html" />
            <item name="Usage" href="./usage.html" />
        </menu>
        <menu ref="reports" />
    </body>

    <!--
    Read more about date format:
    @see http://download.oracle.com/javase/1.5.0/docs/api/java/text/SimpleDateFormat.html
    -->
    <publishDate position="right" format="dd-MMM-yyyy h:mmaa" />
    <version position="right"/>

</project>
